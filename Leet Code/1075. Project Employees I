-- select cte.project_id,round(avg(cte.experience_years),2) as average_years
-- #round(sum(cte.experience_years)/count(*),2) as average_years
-- from cte
-- group by project_id;

SELECT p.project_id, ROUND(AVG(e.experience_years),2) AS average_years
FROM Project p
LEFT JOIN Employee e
ON p.employee_id = e.employee_id
GROUP BY p.project_id
